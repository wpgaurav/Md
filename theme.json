	"appearanceTools": true,
	"spacing": {
		"blockGap": true,
        "margin": true,
        "padding": true,
        "units": ["%","px","em","rem","vh","vw"],
        "spacingScale": {
            "steps": 0
        },
        "spacingSizes": [
            {
                "size": "clamp(0.5rem, 2vw, 0.7rem)",
                "slug": "30",
                "name": "1"
            },
            {
                "size": "clamp(0.6rem, 0.6rem + ((1vw - 0.48rem) * 1.65), 1rem)",
                "slug": "40",
                "name": "2"
            },
            {
                "size": "clamp(1rem, 3vw, 2.5rem)",
                "slug": "50",
                "name": "3"
            },
            {
                "size": "clamp(1.25rem, 3.33vw, 3.33rem)",
                "slug": "60",
                "name": "4"
            },
            {
                "size": "clamp(2rem, 2.25rem + ((1vw - 0.48rem) * 3.096), 4rem)",
                "slug": "70",
                "name": "5"
            },
            {
                "size": "clamp(3rem, 7vw, 6rem)",
                "slug": "80",
                "name": "6"
            }
        ]
        },
	"layout": {
        "contentSize": "750px",
        "wideSize": "960px"
      },
	"typography": {
		  "customFontSize": true,
		  "customLineHeight": true,
		  "dropCap": true,
		  "fluid": true,
		"fontFamilies": [
				{
					"fontFamily": "SF Pro,ui-sans-serif,sans-serif",
					"name": "System",
					"slug": "primary",
					"fontFace": []
				},
				{
					"fontFamily": "monospace",
					"name": "Monospace",
					"slug": "monospace",
					"fontFace": []
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "0.7rem",
						"max": "0.9rem"
					},
					"size": "0.8rem",
					"slug": "x-small",
					"name": "Extra Small"
				},
				{
					"fluid": {
						"min": "0.95rem",
						"max": "1.05rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1.1rem",
						"max": "1.3rem"
					},
					"size": "1.2rem",
					"slug": "base",
					"name": "Base"
				},
				{
					"fluid": {
						"min": "1.2rem",
						"max": "1.6rem"
					},
					"size": "1.4rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.6rem",
						"max": "2rem"
					},
					"size": "1.75rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.8rem"
					},
					"size": "2.4rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.8rem",
						"max": "3.8rem"
					},
					"size": "3.3rem",
					"slug": "xx-large",
					"name": "2xl"
				},
				{
					"fluid": {
						"min": "3rem",
						"max": "4.8rem"
					},
					"size": "4rem",
					"slug": "xxx-large",
					"name": "3xl"
				}
			],
			"lineHeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
    "styles": {
		"blocks": {
			"core/paragraph": {
				"typography": {
					"lineHeight": "calc(1.7em + .2vw)"
				}
			},
			"core/heading": {
				"typography": {
					"lineHeight": "calc(1em + .2vw)"
				}
			},
			"core/button": {
                "typography": {
                    "fontWeight": "700"
                }
            },
			"core/buttons": {
                "spacing": {
                    "blockGap": "10px"
                }
            },
			"core/code": {
                "border": {
                    "radius": "0",
                    "width": "0"
                },
                "color": {
                    "background": "var(--wp--preset--color--bg-highlight)",
                    "text": "var(--wp--preset--color--white)"
                },
				"spacing": {
					"margin": {
						"bottom": "1em"
					},
					"padding": {
						"top": "2em",
						"bottom": "2em",
						"left":"1em",
						"right":"1em"
					}
				}
			},
			"core/group": {
				"spacing": {
					"padding": {
						"top": "2em",
						"bottom": "2em",
						"left":"1em",
						"right":"1em"
					}
				}
			},
			"core/cover": {
				"border": {
                    "radius": "5px",
                    "width": "0"
                },
				"spacing": {
					"margin": {
						"bottom": "1em"
					},
					"padding": {
						"top": "2em",
						"bottom": "2em",
						"left":"1em",
						"right":"1em"
					}
				}
			},
			"core/pullquote": {
				"spacing": {
					"padding": {
						"top": "0",
						"bottom": "0",
						"left":"0",
						"right":"0"
					}
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/image": {
				"spacing": {
					"margin": {
						"top": "1em",
						"bottom":"1em"
					}
				}
			}
	},
      "elements": {
		 "button": {
                "border": {
                    "radius": "4px"
                },
			 "color": {
                "background": "var(--wp--preset--color--tertiary)",
				"text": "var(--wp--preset--color--my-yellow)"
            }
		 },
        "link": {
            "color": {
                "text": "var(--wp--preset--color--primary)"
            }
        }
    }
    }
  }